    "typescript"
  ],
  "dependencies": {
    "cac": "^6.6.1",
    "chalk": "^4.1.0",
    "envinfo": "^7.7.3",
    "sao": "^1.7.1",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-standard": "^14.1.1",
