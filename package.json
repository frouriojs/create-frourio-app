{
  "name": "create-frourio-app",
  "version": "0.30.0",
  "description": "Create a frourio project in seconds",
  "author": "Solufa <solufa2020@gmail.com>",
  "license": "MIT",
  "bin": "bin/index.js",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:server": "webpack --watch --mode=development",
    "dev:aspida": "aspida --watch",
    "dev:frourio": "cd server && frourio --watch",
    "build": "run-p build:client build:server",
    "build:client": "aspida && next build && next export",
    "build:server": "npm run build:frourio && webpack --mode=production",
    "build:frourio": "cd server && frourio",
    "lint": "eslint --ext .ts,.js,.tsx . && prettier --check \"./**/*.ts\"",
    "lint:fix": "eslint --ext .ts,.js,.tsx . --fix && prettier --check \"./**/*.ts\" --write",
    "release": "standard-version --skip.tag",
    "release:major": "npm run release -- --release-as major",
    "release:minor": "npm run release -- --release-as minor",
    "release:patch": "npm run release -- --release-as patch",
    "typecheck": "tsc --noEmit && tsc --noEmit -p server",
    "test": "jest"
  },
  "homepage": "https://frourio.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frouriojs/create-frourio-app.git"
  },
  "bugs": {
    "url": "https://github.com/frouriojs/create-frourio-app/issues"
  },
  "files": [
    "bin",
    "out",
    "server/index.js",
    "server/templates"
  ],
  "keywords": [
    "frourio",
    "typescript"
  ],
  "dependencies": {
    "@aspida/axios": "^1.7.1",
    "@aspida/swr": "^1.7.1",
    "ansi_up": "^5.1.0",
    "axios": "^0.26.0",
    "commander": "^9.0.0",
    "cross-spawn": "^7.0.3",
    "ejs": "^3.1.6",
    "fastify": "^3.27.1",
    "fastify-cors": "^6.0.2",
    "fastify-static": "^4.5.0",
    "fastify-websocket": "^4.0.0",
    "frourio": "^0.26.0",
    "github-buttons": "^2.21.1",
    "is-binary-path": "^2.1.0",
    "mariadb": "^2.5.5",
    "next": "^12.0.10",
    "object-hash": "^2.2.0",
    "open": "^8.4.0",
    "pg": "^8.7.3",
    "portfinder": "^1.0.28",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-flip-toolkit": "^7.0.13",
    "react-markdown": "^8.0.0",
    "react-query": "^3.12.0",
    "real-executable-path": "^3.0.0",
    "swr": "^1.2.2"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/ejs": "^3.1.0",
    "@types/jest": "^27.4.0",
    "@types/jest-expect-message": "^1.0.3",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^17.0.19",
    "@types/object-hash": "^2.2.1",
    "@types/ora": "^3.2.0",
    "@types/pg": "^8.6.4",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/tcp-port-used": "^1.0.0",
    "@types/ws": "^8.2.3",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "fast-glob": "^3.2.11",
    "fastify-nextjs": "^7.2.0",
    "form-data": "^4.0.0",
    "jest": "^27.5.0",
    "jest-expect-message": "^1.0.2",
    "nodemon-webpack-plugin": "^4.7.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "tcp-port-used": "^1.0.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "~4.5.5",
    "wait-for-expect": "^3.0.2",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0",
    "yaml": "^1.10.2"
  }
}
