    "form-data": "^4.0.1",
    "frourio": "^1.3.1",
    "globals": "^15.14.0",
    "jiti": "^2.4.2",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tcp-port-used": "^1.0.2",
