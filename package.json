{
  "name": "create-frourio-app",
  "version": "0.0.0",
  "description": "Create a frourio project in seconds",
  "author": "Solufa <solufa2020@gmail.com>",
  "license": "MIT",
  "bin": "bin/index.js",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:server": "webpack --watch --mode=development",
    "dev:aspida": "aspida --watch",
    "dev:frourio": "cd server && frourio --watch",
    "build": "run-p build:client build:server",
    "build:client": "aspida && next build && next export",
    "build:server": "npm run build:frourio && webpack --mode=production",
    "build:frourio": "cd server && frourio",
    "lint": "eslint --ext .ts,.js,.tsx . && prettier --check \"./**/*.ts\"",
    "release": "standard-version --skip.tag",
    "release:major": "npm run release -- --release-as major",
    "release:minor": "npm run release -- --release-as minor",
    "release:patch": "npm run release -- --release-as patch",
    "typecheck": "tsc --noEmit && tsc --noEmit -p server",
    "test": "jest"
  },
  "homepage": "https://frourio.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frouriojs/create-frourio-app.git"
  },
  "bugs": {
    "url": "https://github.com/frouriojs/create-frourio-app/issues"
  },
  "files": [
    "bin",
    "out",
    "server/index.js",
    "server/templates"
  ],
  "keywords": [
    "frourio",
    "typescript"
  ],
  "dependencies": {
    "@aspida/axios": "^1.7.1",
    "@aspida/swr": "^1.7.1",
    "ansi_up": "^5.0.1",
    "axios": "^0.21.1",
    "cross-spawn": "^7.0.3",
    "ejs": "^3.1.6",
    "fastify": "^3.15.0",
    "fastify-cors": "^5.2.0",
    "fastify-static": "^4.0.1",
    "fastify-websocket": "^3.1.0",
    "frourio": "^0.26.0",
    "github-buttons": "^2.17.0",
    "is-binary-path": "^2.1.0",
    "mariadb": "^2.5.3",
    "next": "^10.1.3",
    "object-hash": "^2.1.1",
    "open": "^8.0.6",
    "pg": "^8.6.0",
    "portfinder": "^1.0.28",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-flip-toolkit": "^7.0.13",
    "react-markdown": "^6.0.0",
    "real-executable-path": "^3.0.0",
    "swr": "^0.5.5"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/ejs": "^3.0.6",
    "@types/jest": "^26.0.22",
    "@types/jest-expect-message": "^1.0.3",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.14.41",
    "@types/object-hash": "^2.1.0",
    "@types/ora": "^3.2.0",
    "@types/pg": "^7.14.11",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/tcp-port-used": "^1.0.0",
    "@types/ws": "^7.4.1",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-react": "^7.23.2",
    "fast-glob": "^3.2.5",
    "fastify-nextjs": "^5.3.0",
    "form-data": "^4.0.0",
    "jest": "^26.6.3",
    "jest-expect-message": "^1.0.2",
    "nodemon-webpack-plugin": "^4.5.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "standard-version": "^9.2.0",
    "tcp-port-used": "^1.0.2",
    "ts-jest": "^26.5.5",
    "ts-loader": "^9.1.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.4",
    "wait-for-expect": "^3.0.2",
    "webpack": "^5.35.0",
    "webpack-cli": "^4.6.0",
    "webpack-node-externals": "^3.0.0",
    "yaml": "^1.10.2"
  }
}
