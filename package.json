{
  "name": "create-frourio-app",
  "version": "0.16.0",
  "description": "Create a frourio project in seconds",
  "author": "Solufa <solufa2020@gmail.com>",
  "license": "MIT",
  "bin": "bin/index.js",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:front": "next dev",
    "dev:server": "webpack --watch --mode=development",
    "dev:aspida": "aspida --watch",
    "dev:frourio": "cd server && frourio --watch",
    "build": "run-p build:front build:server",
    "build:front": "aspida && next build && next export",
    "build:server": "npm run build:frourio && webpack --mode=production",
    "build:frourio": "cd server && frourio",
    "build:types": "aspida && npm run build:frourio",
    "lint": "eslint --ext .ts,.js,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "release": "standard-version --skip.tag",
    "release:major": "npm run release -- --release-as major",
    "release:minor": "npm run release -- --release-as minor",
    "release:patch": "npm run release -- --release-as patch",
    "typecheck": "tsc --noEmit && tsc --noEmit -p server",
    "test": "jest --passWithNoTests",
    "test:update": "cross-env TEST_MODE=update webpack --mode=production && ts-node server/tests/update.ts"
  },
  "homepage": "https://github.com/frouriojs/create-frourio-app#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frouriojs/create-frourio-app.git"
  },
  "bugs": {
    "url": "https://github.com/frouriojs/create-frourio-app/issues"
  },
  "files": [
    "bin",
    "out",
    "server/index.js",
    "server/generator/saofile.js",
    "server/templates"
  ],
  "keywords": [
    "frourio",
    "typescript"
  ],
  "dependencies": {
    "@aspida/axios": "^0.12.1",
    "@aspida/swr": "^0.2.1",
    "axios": "^0.21.0",
    "cross-spawn": "^7.0.3",
    "fastify": "^3.7.0",
    "fastify-cors": "^4.1.0",
    "fastify-static": "^3.3.0",
    "frourio": "^0.18.1",
    "github-buttons": "^2.14.0",
    "next": "^9.5.5",
    "open": "^7.3.0",
    "portfinder": "^1.0.28",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sao": "^1.7.1",
    "swr": "^0.3.6",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@types/validate-npm-package-name": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "jest": "^26.6.1",
    "nodemon-webpack-plugin": "^4.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.2",
    "ts-loader": "^8.0.7",
    "ts-node": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.0.3",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-node-externals": "^2.5.2"
  }
}
