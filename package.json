{
  "name": "create-frourio-app",
  "version": "0.22.1",
  "description": "Create a frourio project in seconds",
  "author": "Solufa <solufa2020@gmail.com>",
  "license": "MIT",
  "bin": "bin/index.js",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:client": "next dev",
    "dev:server": "webpack --watch --mode=development",
    "dev:aspida": "aspida --watch",
    "dev:frourio": "cd server && frourio --watch",
    "build": "run-p build:client build:server",
    "build:client": "aspida && next build && next export",
    "build:server": "npm run build:frourio && webpack --mode=production",
    "build:frourio": "cd server && frourio",
    "lint": "eslint --ext .ts,.js,.tsx .",
    "release": "standard-version --skip.tag",
    "release:major": "npm run release -- --release-as major",
    "release:minor": "npm run release -- --release-as minor",
    "release:patch": "npm run release -- --release-as patch",
    "snapshot": "ts-node server/snapshot/index.ts",
    "typecheck": "tsc --noEmit && tsc --noEmit -p server",
    "test": "jest"
  },
  "homepage": "https://frourio.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frouriojs/create-frourio-app.git"
  },
  "bugs": {
    "url": "https://github.com/frouriojs/create-frourio-app/issues"
  },
  "files": [
    "bin",
    "out",
    "server/index.js",
    "server/templates"
  ],
  "keywords": [
    "frourio",
    "typescript"
  ],
  "dependencies": {
    "@aspida/axios": "^1.1.1",
    "@aspida/swr": "^1.1.1",
    "axios": "^0.21.0",
    "cross-spawn": "^7.0.3",
    "ejs": "^3.1.5",
    "fastify": "^3.9.2",
    "fastify-cors": "^5.1.0",
    "fastify-static": "^3.3.0",
    "frourio": "^0.21.4",
    "github-buttons": "^2.14.1",
    "is-binary-path": "^2.1.0",
    "log-update": "^4.0.0",
    "majo": "^0.10.1",
    "mariadb": "^2.5.2",
    "next": "^10.0.3",
    "open": "^7.3.0",
    "ora": "^5.1.0",
    "pg": "^8.5.1",
    "portfinder": "^1.0.28",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "swr": "^0.3.9"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/ejs": "^3.0.5",
    "@types/jest": "^26.0.19",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.14.14",
    "@types/ora": "^3.2.0",
    "@types/pg": "^7.14.7",
    "@types/react": "^17.0.0",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "jest": "^26.6.3",
    "nodemon-webpack-plugin": "^4.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  }
}
