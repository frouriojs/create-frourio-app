    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "swr": "^2.3.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@types/ejs": "^3.1.5",
    "@types/eslint-config-prettier": "^6.11.3",
