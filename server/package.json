{
  "scripts": {
    "dev": "frourio && run-p dev:*",
    "dev:server": "onchange -i -k ./build/index.dev.js -- node ./build/index.dev.js --host=0.0.0.0",
    "dev:build-server-dev": "ts-node -T ./scripts/build-file.ts --from=./index.ts --to=./build/index.dev.js --target=es2019 --server --watch",
    "dev:aspida": "aspida --watch",
    "dev:frourio": "frourio --watch",
    "build": "frourio && ts-node -T ./scripts/build-file.ts --from=./index.ts --to=./build/index.js --target=es2019 --server --prod",
    "test": "vitest run",
    "typecheck": "tsc --pretty --noEmit"
  }
}
