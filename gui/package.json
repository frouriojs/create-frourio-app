{
  "name": "cfa-gui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:front": "next dev",
    "dev:server": "webpack --watch --mode=development",
    "dev:aspida": "aspida --watch",
    "dev:frourio": "cd server && frourio --watch",
    "build": "run-p build:front build:server",
    "build:front": "aspida && next build && next export",
    "build:server": "npm run build:frourio && webpack --mode=production",
    "build:frourio": "cd server && frourio",
    "build:types": "aspida && npm run build:frourio",
    "lint": "eslint --ext .ts,.js,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "npm run build:types && tsc --noEmit && tsc --noEmit -p server",
    "test": "npm run build:types && jest"
  },
  "dependencies": {
    "@aspida/axios": "^0.12.0",
    "@aspida/swr": "^0.2.0",
    "axios": "^0.20.0",
    "cross-spawn": "^7.0.3",
    "fastify": "^3.6.0",
    "fastify-cors": "^4.1.0",
    "fastify-static": "^3.2.1",
    "frourio": "^0.17.2",
    "next": "9.5.3",
    "open-editor": "^2.0.1",
    "portfinder": "^1.0.28",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sao": "^1.7.1",
    "swr": "^0.3.5",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@types/validate-npm-package-name": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "jest": "^26.4.2",
    "nodemon-webpack-plugin": "^4.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.5",
    "ts-node": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.0.3",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-node-externals": "^2.5.2"
  }
}