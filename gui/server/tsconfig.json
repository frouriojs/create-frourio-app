{
  "compilerOptions": {
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "lib": ["dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "$/*": ["./*"]
    },
    "rootDir": ".",
    "strict": true,
    "strictPropertyInitialization": false,
    "target": "es6"
  }
}
